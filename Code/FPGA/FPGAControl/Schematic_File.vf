                             .Motor_Tilt_hall(XLXN_44[8:0]), 
                             .Stop(XLXN_36[1:0]));
   Hall_sensor  XLXI_13 (.Hall_sens(Hall_sens[1:0]), 
                        .Pan_Hall(XLXN_21), 
                        .Tilt_Hall(XLXN_30));
   Motor_Pan  XLXI_20 (.Clk(clk), 
                      .Encoder_Pan(Encoder_Pan[1:0]), 
                      .Sync_Hall(XLXN_30), 
                      .Encoder_Register());
   LED_Blinky  XLXI_21 (.Input(XLXN_36[1:0]), 
                       .Position(Position[7:0]));
   Clock_scale  XLXI_22 (.Clk(clk), 
                        .Clk_100hz(), 
                        .Fake_Encoder_pan(XLXN_44[8:0]), 
                        .Fake_Encoder_Tilt(XLXN_45[10:0]));
endmodule
