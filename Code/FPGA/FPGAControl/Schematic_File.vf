`timescale 1ns / 1ps

module Schematic_File(clk, 
                      Encoder_Pan, 
                      Encoder_tilt, 
                      Hall_sens, 
                      Position);

    input clk;
    input [1:0] Encoder_Pan;
    input [1:0] Encoder_tilt;
    input [1:0] Hall_sens;
   output [7:0] Position;
   
   wire XLXN_21;
   wire XLXN_30;
   wire [1:0] XLXN_36;
   wire [8:0] XLXN_44;
   wire [10:0] XLXN_45;
   
   Motor_Tilt  XLXI_5 (.Clk(clk), 
                      .Encoder_tilt(Encoder_tilt[1:0]), 
                      .Sync_Hall(XLXN_21), 
                      .Encoder_Register());
