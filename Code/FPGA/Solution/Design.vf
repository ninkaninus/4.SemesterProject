`timescale 1ns / 1ps

module Design(clk, 
              encoder1, 
              encoder2, 
              SPI_Clk, 
              SPI_MOSI, 
              SPI_SS, 
              sw, 
              Enable, 
              led, 
              motor, 
              seg, 
              SPI_MISO);

    input clk;
    input encoder1;
    input encoder2;
    input SPI_Clk;
    input SPI_MOSI;
    input SPI_SS;
    input [7:0] sw;
   output [3:0] Enable;
   output [7:0] led;
   output [1:0] motor;
   output [0:7] seg;
   output SPI_MISO;
   
   wire [11:0] DataBus;
   wire XLXN_89;
   wire [15:0] XLXN_138;
   wire XLXN_148;
   wire [3:0] XLXN_149;
   
   SPI_Slave3  XLXI_21 (.Clk(clk), 
