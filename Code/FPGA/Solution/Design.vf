                            .motor(motor[1:0]));
   pwm_driver  XLXI_26 (.AdrBus(XLXN_149[3:0]), 
                       .clk(clk), 
                       .DataBus(DataBus[7:0]), 
                       .WE(XLXN_148), 
                       .pwm(XLXN_89));
   Blok1  XLXI_27 (.AdrBus(XLXN_149[3:0]), 
                  .Clk(clk), 
                  .Sw(sw[7:0]), 
                  .WE(XLXN_148), 
                  .Led(led[7:0]), 
