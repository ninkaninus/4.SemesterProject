                           .Encoder2(encoder2), 
                           .WE(XLXN_148), 
                           .Bcd(XLXN_138[15:0]));
   Motor_Interface  XLXI_25 (.AdrBus(XLXN_149[3:0]), 
                            .clk(clk), 
                            .DataBus(DataBus[1:0]), 
